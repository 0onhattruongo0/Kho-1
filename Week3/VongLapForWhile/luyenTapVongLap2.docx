<script>
    let n = 5;
    for (let i = 1 ; i <= n ; i++){
        for (let j = 1 ; j <=n ; j++){
            if (j<=n-i){
                document.write('&nbsp&nbsp');
            } else {
                document.write('*');
            }
        }document.write('<br>');
    }
    
  
</script>

